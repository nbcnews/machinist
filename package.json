{
  "name": "@nbcnews/machinist",
  "version": "1.0.8",
  "description": "A boilerplate for a Metalsmith static site.",
  "main": "bin/cli",
  "scripts": {
    "secure-check": "node bin/secureCheck",
    "test": "npm link && machinist",
    "start": "BUILD=development node bin/cli --build",
    "doc-ingest": "node ./lib/googledocjson.js && node ./lib/dropboxpaperjson.js",
    "googlesheet-ingest": "node ./lib/googlesheetjson.js",
    "dev": "npm start",
    "dev:debug": "DEBUG=machinist:*,metalsmith:* npm start",
    "build": "BUILD=production node bin/cli --build",
    "build:debug": "DEBUG=machinist:*,metalsmith:* BUILD=production node bin/cli --build",
    "format": "eslint \"src/scripts/**\" \"lib/**\" --fix && stylelint \"src/styles/**/*.scss\" --fix",
    "publish:cdn": "node bin/cli --publish assets",
    "publish": "npm run build && node bin/cli --publish story",
    "script-lint": "eslint \"src/scripts/**\" \"lib/**\"",
    "style-lint": "stylelint \"**/*.scss\"",
    "htmlhint": "htmlhint **/www/**/*.html --config .htmlhintrc",
    "lint": "npm run script-lint && npm run style-lint && npm run htmlhint",
    "precommit": "lint-staged"
  },
  "bin": {
    "machinist": "bin/cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nbcnews/machinist.git"
  },
  "keywords": [
    "metalsmith",
    "boilerplate",
    "static",
    "site"
  ],
  "author": "nbcnews",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nbcnews/machinist/issues"
  },
  "homepage": "https://github.com/nbcnews/machinist#readme",
  "lint-staged": {
    "*.scss": "style-lint",
    "*.js": "script-lint"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.38",
    "@babel/preset-env": "^7.0.0-beta.38",
    "archieml": "^0.4.2",
    "async": "^2.6.0",
    "autoprefixer": "^7.2.5",
    "babel-loader": "^8.0.0-beta.0",
    "commander": "2.10.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "git-assert-clean": "^1.0.0",
    "googledoc-to-json": "^0.3.0",
    "googlesheet-to-json": "0.0.1",
    "gulp": "^3.9.1",
    "gulp-awspublish": "^3.3.0",
    "gulp-filter": "^5.0.1",
    "gulp-jsoncombine": "^1.0.3",
    "gulp-md5-plus": "^1.0.3",
    "gulp-prompt": "^0.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-rewrite-image-path": "0.0.4",
    "handlebars": "^4.0.5",
    "handlebars-helpers": "^0.7.5",
    "js-yaml": "^3.10.0",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-markdown": "^1.2.1",
    "log-utils": "^0.2.1",
    "metalsmith": "^2.2.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-browser-sync": "^1.1.1",
    "metalsmith-debug": "^1.1.0",
    "metalsmith-debug-ui": "^0.2.0",
    "metalsmith-discover-partials": "^0.1.0",
    "metalsmith-drafts": "0.0.1",
    "metalsmith-filedata": "^0.1.0",
    "metalsmith-fingerprint-ignore": "^1.1.2",
    "metalsmith-in-place": "^4.1.1",
    "metalsmith-layouts": "^2.1.0",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-markdown": "^0.2.2",
    "metalsmith-metadata": "0.0.4",
    "metalsmith-paths": "^3.0.1",
    "metalsmith-permalinks": "^0.5.0",
    "metalsmith-raw": "^0.2.0",
    "metalsmith-register-helpers": "^0.4.0",
    "metalsmith-writemetadata": "^0.4.5",
    "minimatch": "^3.0.4",
    "moment": "^2.18.1",
    "node-sass": "^4.7.2",
    "paper2json": "^0.2.2",
    "postcss": "^6.0.16",
    "read-metadata": "^1.0.0",
    "typesettings": "^4.0.0",
    "typogr": "^0.6.6",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "uniq": "^1.0.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.1.1",
    "eslint": "^3.19.0",
    "htmlhint": "^0.9.13",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "standard": "^10.0.2",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0"
  }
}
